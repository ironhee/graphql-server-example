{
  "name": "graphql-server-example",
  "scripts": {
    "test": "$(npm bin)/mocha $npm_package_options_mocha",
    "test-watch": "npm run test -- --watch",
    "server": "$(npm bin)/babel-node tools/run graphqlServer",
    "build": "$(npm bin)/babel-node tools/run.js build",
    "build-watch": "$(npm bin)/babel-node tools/run.js serve",
    "update-schema": "$(npm bin)/babel-node tools/run.js updateSchema",
    "start": "npm run server && npm run build-watch",
    "lint": "$(npm bin)/eslint server client"
  },
  "options": {
    "mocha": "--stage 0 --compilers js:babel/register server/**/__tests__/**/*.js"
  },
  "metadata": {
    "graphql": {
      "schema": "./data/schema.json"
    }
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "5.3.2",
    "babel-relay-plugin": "^0.3.0",
    "babel-relay-plugin-loader": "^0.3.1",
    "chai": "^3.4.0",
    "del": "^2.0.2",
    "eslint": "^1.7.2",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "gaze": "^0.5.2",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "ncp": "^2.0.0",
    "react-hot-loader": "^2.0.0-alpha-2",
    "sinon": "^1.17.2",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "express-graphql": "^0.4.0",
    "graphql": "^0.4.12",
    "graphql-relay": "^0.3.3",
    "history": "^1.13.0",
    "jwt-simple": "^0.3.1",
    "lodash": "^3.10.1",
    "lowdb": "^0.10.2",
    "node-uuid": "^1.4.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-relay": "^0.4.0",
    "react-router": "^1.0.0-rc4",
    "react-router-relay": "^0.7.0",
    "rethinkdb": "^2.1.1",
    "thinky": "^2.1.9",
    "underscore-db": "^0.9.1"
  }
}
